   "source": [
    "import numpy as np\n",
    "\n",
    "def gini(y: np.ndarray):\n",
    "    p = np.unique(y, return_counts=True)[1]/len(y)\n",
    "    return np.sum(p * (1 - p))"
   ]
  },

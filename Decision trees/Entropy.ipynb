   "source": [
    "import numpy as np\n",
    "\n",
    "def entropy(y: np.ndarray):\n",
    "    p = np.unique(y, return_counts=True)[1]/len(y)\n",
    "    return -np.sum(p * np.log(p))\n",
    "    "
   ]
  },
